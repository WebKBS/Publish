@{
    ViewData["title"] = "SIGN UP";
}

<div class="top_title_wrap">
    <div class="inner">
        <h2 class="subtitle_1 suit_m">SIGN UP</h2>
        <p class="sub_title body_2 color_white_2 suit_r">AIR DROPPER is optimized for ‘Chrome’ web browser and does not support mobile service.</p>
    </div>
</div>

<section class="cont sign_up_wrap">
    <div class="inner">
        <h2 class="subtitle_1 suit_m">Agree to the Terms & Conditions</h2>
        <div class="form_wrap">
            <div class="all_select_wrap">
                <input id="all_check" type="checkbox" />
                <label for="all_check">
                    <img src="~/images/icons/check.svg" alt="Agree to all terms and conditions" />
                    <img src="~/images/icons/check-mark2-off.svg" alt="Agree to all terms and conditions" />
                    <span class="body_2 suit_m">I agree to all terms and conditions below.</span>
                </label>
            </div>
            <div class="list">
                <ul>
                    <li>
                        <div class="check_wrap">
                            <input type="checkbox" id="check1" data-required="true" />
                            <label for="check1">
                                <img src="~/images/icons/check.svg" alt="(Required) Agree to Terms of Use" />
                                <img src="~/images/icons/check-mark2-off.svg" alt="(Required) Agree to Terms of Use" />
                                <span class="body_2 suit_r color_white_2">(Required) Agree to Terms of Use</span>
                            </label>
                        </div>
                        <a href="#" class="detail body_2 suit_r color_white_3">
                            View Details
                        </a>
                    </li>
                    <li>
                        <div class="check_wrap">
                            <input type="checkbox" id="check2" data-required="true" />
                            <label for="check2">
                                <img src="~/images/icons/check.svg" alt="(Required) Agree to Privacy Policy" />
                                <img src="~/images/icons/check-mark2-off.svg" alt="(Required) Agree to Privacy Policy" />
                                <span class="body_2 suit_r color_white_2">(Required) Agree to Privacy Policy</span>
                            </label>
                        </div>
                        <a href="#" class="detail body_2 suit_r color_white_3">
                            View Details
                        </a>
                    </li>
                    <li>
                        <div class="check_wrap">
                            <input type="checkbox" id="check3" data-required="true" />
                            <label for="check3">
                                <img src="~/images/icons/check.svg" alt="(Required) Agree to provision of personal information to third parties" />
                                <img src="~/images/icons/check-mark2-off.svg" alt="(Required) Agree to provision of personal information to third parties" />
                                <span class="body_2 suit_r color_white_2">(Required) Agree to provision of personal information to third parties</span>
                            </label>
                        </div>
                        <a href="#" class="detail body_2 suit_r color_white_3">
                            View Details
                        </a>
                    </li>
                    <li>
                        <div class="check_wrap">
                            <input type="checkbox" id="check4" data-required="false" />
                            <label for="check4">
                                <img src="~/images/icons/check.svg" alt="(Optional) Agree to receive marketing information" />
                                <img src="~/images/icons/check-mark2-off.svg" alt="(Optional) Agree to receive marketing information" />
                                <span class="body_2 suit_r color_white_2">(Optional) Agree to receive marketing information</span>
                            </label>
                        </div>
                        <a href="#" class="detail body_2 suit_r color_white_3">
                            View Details
                        </a>
                    </li>
                </ul>
            </div>
        </div>
        <button type="button" class="next_btn button_160" disabled>Next</button>
    </div>
</section>

